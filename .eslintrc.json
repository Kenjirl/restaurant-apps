{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-underscore-dangle":"off",
        "no-restricted-globals":"off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/webpack.*.js"]}]
    }
}
